<AnchorPane id="AnchorPane" fx:id="frmTelaLogin" prefHeight="350.0" prefWidth="600.0" style="-fx-background-color: ffffff;" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.TelaLoginController">
